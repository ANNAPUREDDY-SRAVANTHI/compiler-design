#include <stdio.h>
int evaluateExpression()
{
    int result = (2 + 3) * 4 / 2 - 1;
    return result;
}
int main()
{
    printf("PEMDAS Rule Demonstration\n"); printf("Expression: (2 + 3) * 4 / 2 - 1\n");
    printf("Evaluation follows: Parentheses -> Multiplication/Division -> Addition/Subtraction\n");
    printf("Result: %d\n", evaluateExpression());
    return 0;
}
